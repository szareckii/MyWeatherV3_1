<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/feedback_textView"
        style="@style/Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/feedback"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209" />

    <TextView
        android:id="@+id/phone_textView"
        style="@style/Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="@string/phone"
        app:layout_constraintEnd_toEndOf="@+id/feedback_textView"
        app:layout_constraintStart_toStartOf="@+id/feedback_textView"
        app:layout_constraintTop_toBottomOf="@+id/feedback_textView" />

    <TextView
        android:id="@+id/email_textView"
        style="@style/Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="@string/email"
        app:layout_constraintEnd_toEndOf="@+id/phone_textView"
        app:layout_constraintStart_toStartOf="@+id/phone_textView"
        app:layout_constraintTop_toBottomOf="@+id/phone_textView" />
</androidx.constraintlayout.widget.ConstraintLayout>